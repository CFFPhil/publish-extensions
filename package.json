{
  "name": "publish-to-open-vsx",
  "version": "0.0.0",
  "description": "CI for publishing VS Code extensions to open-vsx.org",
  "homepage": "https://cloudstudioopen-openvsx-wzroc3u5ppc.ws-us94.gitpod.io",
  "license": "EPL-2.0",
  "bugs": {
    "url": "https://github.com/CloudStudioOpenvsx/publish-extensions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudStudioOpenvsx/publish-extensions.git"
  },
  "contributors": [
    {
      "name": "TypeFox",
      "url": "https://www.typefox.io"
    },
    {
      "name": "Gitpod",
      "url": "https://www.gitpod.io"
    }
  ],
  "bin": "bin/publish-extensions",
  "main": "publish-extensions",
  "scripts": {
    "publish": "node publish-extensions"
  },
  "dependencies": {
    "@vscode/vsce": "^2.18.0",
    "ajv": "^8.8.1",
    "download": "^8.0.0",
    "fast-glob": "^3.2.12",
    "find-up": "^5.0.0",
    "human-number": "^2.0.0",
    "minimist": "^1.2.5",
    "octokit": "^2.0.2",
    "ovsx": "latest",
    "semver": "^7.1.3",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/unzipper": "^0.10.5"
  },
  "publish-extensions": {
    "targets": [
      "vscode"
    ],
    "extensions": [
      {
        "name": "vscode-deno",
        "displayName": "My Extension",
        "description": "This is my awesome extension",
        "version": "3.17.0",
        "publisher": "denoland",
        "vsix": "/tmp/download/vscode-deno.vsix"
      }
    ],
    "registryOptions": {
      "registryUrl": "https://cloudstudioopen-openvsx-wzroc3u5ppc.ws-us94.gitpod.io",
      "publisher": "my-publisher",
      "namespace": "CloudStudio",
      "pat": "${{ secrets.OVSX_PAT }}"
    }
  }
}
